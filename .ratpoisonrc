exec xscreensaver
exec redshift
exec emacs --daemon
startup_message on

bind b exec ~/rat_bat_stat.sh
bind q exec conkeror
bind e exec emacsclient -c
bind g exec gimp

bind C-l nextscreen
bind C-h prevscreen
bind l focusright
bind h focusleft
bind j focusdown
bind k focusup

definekey top XF86AudioRaiseVolume exec amixer set Master 5%+
definekey top XF86AudioLowerVolume exec amixer set Master 5%-
definekey top XF86AudioMute exec amixer set Master toggle
